{
    "name": "web-myna",
    "version": "0.0.9",
    "description": "emulate your api for testing and development",
    "main": "src/index.js",
    "bin": {
        "webmyna": "dist/index.js"
    },
    "files": [
        "dist/",
        "docs/",
        "src/"
    ],
    "repository": "https://github.com/marmelab/web-myna",
    "author": "Alexis Janvier <alexis@marmelab.com> (https://www.marmelab.com)",
    "license": "MIT",
    "private": false,
    "type": "module",
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "start": "nodemon --exec \"node --experimental-modules\" --watch src src/index.js",
        "lint:js": "eslint ./src --ext .js",
        "jsdoc": "jsdoc2md src/**/*.js > ./docs/references.md",
        "build": "parcel build cli/index.js --target node  --no-source-maps && git add dist"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": [
            "eslint",
            "git add"
        ]
    },
    "dependencies": {
        "boxen": "4.1.0",
        "chalk": "2.4.2",
        "clear": "0.1.0",
        "clui": "0.3.6",
        "convict": "5.2.0",
        "express": "4.17.1",
        "figlet": "1.2.4",
        "http-proxy-middleware": "0.20.0",
        "inquirer": "7.0.0",
        "md5": "2.2.1",
        "minimist": "1.2.0",
        "nodemon": "1.19.4",
        "query-string": "6.8.3",
        "rc": "1.2.8",
        "signale": "1.4.0"
    },
    "devDependencies": {
        "@babel/core": "7.7.0",
        "@babel/preset-env": "7.7.1",
        "babel-eslint": "10.0.3",
        "babel-jest": "24.9.0",
        "docusaurus-init": "1.14.0",
        "eslint": "6.6.0",
        "eslint-config-prettier": "6.5.0",
        "eslint-plugin-jest": "23.0.2",
        "eslint-plugin-jsdoc": "17.1.0",
        "eslint-plugin-prettier": "3.1.1",
        "husky": "3.0.9",
        "jest": "24.9.0",
        "jsdoc-to-markdown": "5.0.2",
        "lint-staged": "9.4.2",
        "parcel-bundler": "1.12.4",
        "parcel-plugin-shebang": "1.2.8",
        "prettier": "1.18.2"
    }
}
