{
    "parser": "babel-eslint",
    "plugins": [
        "prettier",
        "jest",
        "jsdoc"
    ],
    "extends": [
        "prettier",
        "prettier/babel",
        "plugin:jest/recommended",
        "plugin:jsdoc/recommended"
    ],
    "rules": {
        "prettier/prettier": [
            "error"
        ],
        "object-curly-spacing": [
            "error",
            "always"
        ],
        "array-bracket-spacing": [
            "error",
            "never"
        ]
    }
}
